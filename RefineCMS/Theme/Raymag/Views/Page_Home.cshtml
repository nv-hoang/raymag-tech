                <div class="inline-block text-[64px] lg:text-[100px] leading-100 font-semibold" style="font-family: 'Inter';">
                    <div class="ani-item" data-classes="gradient-2">@Html.Raw(output.CustomField("Products.BGTitle"))</div>
