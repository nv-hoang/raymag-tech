            <h6 class="ani-item-2 text-[16px] lg:text-[18px] leading-175 text-gray-100 mt-5">@Html.Raw(output.CustomField("Products.Desc", true, true))</h6>

            @if (!string.IsNullOrEmpty(output.CustomField("Products.ButtonLink.Link")))
            {
                <div class="mt-10 ani-item-2">
                    <a href="@output.CustomField("Products.ButtonLink.Link")" data-fancybox class="inline-block rounded-full overflow-hidden gradient-btn-1 cursor-pointer relative">
                        <div class="inline-flex items-center gap-x-[10px] px-10 py-[10px] relative">
                            <div class="text-[16px] leading-175 font-medium text-white">@Html.Raw(output.CustomField("Products.ButtonLink.Label", true))</div>
